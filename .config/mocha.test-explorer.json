{
    "$schema": "https://json.schemastore.org/mocharc.json",
    "extension": ["ts"],
    "ignore": [],
    "package": "./package.json",
    "node-option": ["import=tsx", "conditions=typedoc-ts"],
    "slow": 500,
    "spec": ["src/**/*.test.ts", "src/**/*.test.tsx"],
    "timeout": 0,
    "watch-files": ["src/**/*.ts", "src/**/*.tsx"]
}
