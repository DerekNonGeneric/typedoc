{
    "$schema": "https://typedoc.org/schema.json",
    "intentionallyNotExported": [
        "SORT_STRATEGIES",
        "_ModelToObject",
        "EventHooksMomento",
        "MarkedPlugin",
        "MeaningKeywords"
    ],
    "sort": [
        "kind",
        "instance-first",
        "required-first",
        "alphabetical-ignoring-documents"
    ],
    "entryPoints": ["../src/index.ts"],
    "name": "TypeDoc",
    "readme": "../site/index.md",
    "projectDocuments": [
        "../site/overview.md",
        "../site/options.md",
        "../site/doc-comments/index.md",
        "../site/external-documents.md",
        "../site/themes.md",
        "../site/plugins.md",
        "../site/declaration-references.md",
        "../site/development/index.md",
        "../CHANGELOG.md",
        "../site/tags.md"
    ],
    "alwaysCreateEntryPointModule": true,
    "excludeExternals": true,
    "excludeInternal": false,
    "excludePrivate": true,
    // "excludeReferences": true,
    "groupReferencesByType": true,
    "jsDocCompatibility": false,
    "treatWarningsAsErrors": false,
    "categorizeByGroup": false,
    "categoryOrder": ["Reflections", "Types", "Comments", "*"],
    "validation": {
        "notExported": true,
        "invalidLink": true,
        "notDocumented": false
    },
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
        "@internal": false
    },
    "searchGroupBoosts": {
        "Documents": 10.0,
        "Classes": 2.0,
        "Interfaces": 2.0,
        "Enumerations": 2.0,
        "Type Aliases": 2.0
    },
    "searchInComments": true,
    "searchInDocuments": true,
    "navigation": {
        "includeCategories": true,
        "includeGroups": false,
        "includeReferences": false
    },
    "includeVersion": true,
    "logLevel": "Verbose",
    "externalSymbolLinkMappings": {
        "@types/markdown-it": {
            "MarkdownIt": "https://markdown-it.github.io/markdown-it/#MarkdownIt"
        }
    }
}
